    "improt threading\n",
    "def func():\n",
    "    global num,lock\n",
    "    lock.acquire()\n",
    "    num=num+1\n",
    "num=10\n",
    "lock=threading.Lock()\n",
    "thread1=threading.Thread(target=func)\n",
    "thread2=threading.Thread(target=func)\n",
    "thread1.start()\n",
    "thread2.start()\n",
    "thread2.join()\n",
    "thread1.join()\n",
    "num"
   ]
